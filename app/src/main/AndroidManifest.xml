<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.isme.shen.sdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SplashActivity"
            android:theme="@style/translucentNoTitle" />
        <activity android:name=".srecycleview.SRecycleViewV1Activity" />
        <activity android:name=".srecycleview.SRecycleViewActivity" />
        <activity android:name=".ui.UiEntranceActivity" />
        <activity android:name=".srecycleview.SRecycleViewV2GearActivity"></activity>
    </application>

</manifest>